<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Stella Sun Studio</title>
    <link href="/static/css/bootstrap.min.css" rel="stylesheet">
    <link href="/static/css/font-awesome.min.css" rel="stylesheet">
    <link href="/static/css/animate.min.css" rel="stylesheet">
    <link href="/static/css/lightbox.css" rel="stylesheet">
    <link href="/static/css/main.css" rel="stylesheet">
    <link href="/static/css/responsive.css" rel="stylesheet">
    <link rel="shortcut icon" href="/static/images/ico/sunstudio.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/static/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/static/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/static/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="/static/images/ico/apple-touch-icon-57-precomposed.png">
</head>

<body>

    <header id="header">
        <div class="navbar navbar-inverse" role="banner">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                                <ul class="nav navbar-nav navbar-right">
                                <% header.forEach(function(category){ %>
                                    <li class="dropdown">
                                        <a title="<%= category.name %>" href="/<%= category.url %>">
                                            <%= category.name %><i class="fa fa-angle-down"></i></a>

                                        <ul role="menu" class="sub-menu">
                                            <% category.pages.forEach(function(page){ %>
                                                <li class="active"> <a title="<%= page.name %>" href="/<%= category.url %>/<%= page.url %>"><span><%= page.name %></span></a></li>
                                                <% }); %>
                                        </ul>
                                    </li>
                                    <% }); %>
                            </ul>
                        <span class="sr-only">Toggle navigation</span>

                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                </div>
                <!--HEADER-->
                <header id="header">
                    <div class="container">
                        <div class="row">
                            <a class="navbar-brand" href="/">
                                <h1><img src="/static/images/logo.png" alt="logo" width="100" height="100"></h1>
                            </a>
                            <ul class="nav navbar-nav navbar-right">
                                <% header.forEach(function(category){ %>
                                    <li class="dropdown">
                                        <a title="<%= category.name %>" href="/<%= category.url %>">
                                            <%= category.name %><i class="fa fa-angle-down"></i></a>

                                        <ul role="menu" class="sub-menu">
                                            <% category.pages.forEach(function(page){ %>
                                                <li class="active"> <a title="<%= page.name %>" href="/<%= category.url %>/<%= page.url %>"><span><%= page.name %></span></a></li>
                                                <% }); %>
                                        </ul>
                                    </li>
                                    <% }); %>
                            </ul>

                        </div>

                </header>
                </div>
            </div>
    </header>